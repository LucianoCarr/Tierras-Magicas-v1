<h1>Reinos</h1>
<div class="container">
   <% realms.forEach(realm => { %>
       <div class="card-body">
           <div class="card">
            <a href="/realm/<%= realm.id %>"><h3><%= realm.name %></h3></a>
               <hr>
               <% if (realm.image.startsWith('http')) { %>
                <a href="/realm/<%= realm.id %>"><img src="<%= realm.image %>" alt="<%= realm.name %>"></a>
            <% } else { %>
                <a href="/realm/<%= realm.id %>"><img src="/img/<%= realm.image || 'tierras-magicas.jpg' %>" alt="<%= realm.name %>"></a>
            <% } %>
           </div>
       </div>
   <% }) %>
</div>